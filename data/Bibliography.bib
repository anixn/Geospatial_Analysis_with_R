  keywords = {CAMELS,journal},
  file = {D\:\\OneDrive\\Reference\\storage\\BEAP2RLS\\Coxon et al. - 2020 - CAMELS-GB hydrometeorological time series and lan.pdf;E\:\\OneDrive\\Desktop\\coxon2020camelsgb-zotero.md}
