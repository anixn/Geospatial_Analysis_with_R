  keywords = {!🟩 finished,p01},
  file = {D\:\\OneDrive\\Reference\\storage\\4TAT2IJX\\Fang et al. - 2013 - The Impact of Potential Land Cover Misclassificati.pdf;D\:\\OneDrive\\Reference\\storage\\JW6LMM6G\\Fang et al. - 2013 - The Impact of Potential Land Cover Misclassificati.pdf}
