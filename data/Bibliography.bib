@article{coxonCAMELSGBHydrometeorologicalTime2020,
  title = {{{CAMELS-GB}}: Hydrometeorological Time Series and Landscape Attributes for 671 Catchments in {{Great Britain}}},
  shorttitle = {{{CAMELS-GB}}},
  author = {Coxon, Gemma and Addor, Nans and Bloomfield, John P. and Freer, Jim and Fry, Matt and Hannaford, Jamie and Howden, Nicholas J. K. and Lane, Rosanna and Lewis, Melinda and Robinson, Emma L. and Wagener, Thorsten and Woods, Ross},
