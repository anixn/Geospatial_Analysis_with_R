@article{singhHydrologicalImpactsClimate2014,
  title = {Hydrological Impacts of Climate Change in Gauged and Ungauged Watersheds of the {{Olifants}} Basin: A Trading-Space-for-Time Approach},
  shorttitle = {Hydrological Impacts of Climate Change in Gauged and Ungauged Watersheds of the {{Olifants}} Basin},
